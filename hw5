from pywebio.input import input as pw_input, slider, FLOAT
from pywebio.output import put_success

ONE_KILO_CALORIE = 0.32541

ENERGY_OSTRIGH_EGG = 118
ENERGY_RABBIT = 197
ENERGY_SEA_BASS = 123
ENERGY_SWEET_RED_PEPPER = 26
ENERGY_PARSLEY = 45
ENERGY_BANANA = 87
ENERGY_WAFFLES = 425
ENERGY_BREAD = 246
ENERGY_PISTACHIOS = 555
ENERGY_KEFIR = 51

total_energy = 0

portion_ostrigh_egg = pw_input(f'–í–≤–µ–¥—ñ—Ç—å –±–∞–∂–∞–Ω—É –ø–æ—Ä—Ü—ñ—é —Å—Ç—Ä–∞—É—Å–∏–Ω–∏—Ö —è—î—Ü—å ü•ö, –≥—Ä–∞–º '
                               f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å {ENERGY_OSTRIGH_EGG} –∫–ö–∞–ª/100–≥—Ä', type=FLOAT)
sum1 = ONE_KILO_CALORIE * ENERGY_OSTRIGH_EGG
portion_ostrich_egg_energy = (float(portion_ostrigh_egg) * ENERGY_OSTRIGH_EGG) / 100
total_energy = total_energy + portion_ostrich_egg_energy
put_success(f'–í–∏ –∑–∞–∫–∞–∑–∞–ª–∏ {portion_ostrigh_egg} –≥—Ä–∞–º—ñ–≤ —Å—Ç—Ä–∞—É—Å–∏–Ω–∏—Ö —è—î—Ü—å ü•ö,\n'
            f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –ø–æ—Ä—Ü—ñ—ñ {portion_ostrigh_egg}\n'
            f'–ó–∞–≥–∞–ª—å–Ω–∞ –Ω–∞–∫–æ–ø–∏—á–µ–Ω–∞ –∫–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {total_energy},\n'
            f'–î–æ —Å–ø–ª–∞—Ç–∏ {sum1} –≥—Ä–Ω')


portion_rabbit = slider(f'–í–≤–µ–¥—ñ—Ç—å –±–∞–∂–∞–Ω—É –ø–æ—Ä—Ü—ñ—é –∫—Ä–æ–ª–∏–∫–∞ üêá, –≥—Ä–∞–º '
                               f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å {ENERGY_RABBIT} –∫–ö–∞–ª/100–≥—Ä', min_value=0, max_value=2500)
sum2 = ONE_KILO_CALORIE * ENERGY_RABBIT + sum1
portion_rabbit_energy = (float(portion_rabbit) * ENERGY_RABBIT) / 100
total_energy = total_energy + portion_rabbit_energy
put_success(f'–í–∏ –∑–∞–∫–∞–∑–∞–ª–∏ {portion_rabbit} –≥—Ä–∞–º—ñ–≤ –∫—Ä–æ–ª–∏–∫–∞ üêá,\n'
            f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –ø–æ—Ä—Ü—ñ—ñ {portion_rabbit}\n'
            f'–ó–∞–≥–∞–ª—å–Ω–∞ –Ω–∞–∫–æ–ø–∏—á–µ–Ω–∞ –∫–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {total_energy},\n'
            f'–í—Å—å–æ–≥–æ –¥–æ —Å–ø–ª–∞—Ç–∏ {sum2} –≥—Ä–Ω')

portion_sea_bass = slider(f'–í–≤–µ–¥—ñ—Ç—å –±–∞–∂–∞–Ω—É –ø–æ—Ä—Ü—ñ—é –æ–∫—É–Ω—è üêü, –≥—Ä–∞–º '
                               f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å {ENERGY_SEA_BASS} –∫–ö–∞–ª/100–≥—Ä', min_value=0, max_value=2500)
sum3 = ONE_KILO_CALORIE * ENERGY_SEA_BASS + sum2
portion_sea_bass_energy = (float(portion_sea_bass) * ENERGY_SEA_BASS) / 100
total_energy = total_energy + portion_sea_bass_energy
put_success(f'–í–∏ –∑–∞–∫–∞–∑–∞–ª–∏ {portion_sea_bass} –≥—Ä–∞–º—ñ–≤ –æ–∫—É–Ω—è üêü,\n'
            f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –ø–æ—Ä—Ü—ñ—ñ {portion_sea_bass}\n'
            f'–ó–∞–≥–∞–ª—å–Ω–∞ –Ω–∞–∫–æ–ø–∏—á–µ–Ω–∞ –∫–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {total_energy},\n'
            f'–í—Å—å–æ–≥–æ –¥–æ —Å–ø–ª–∞—Ç–∏ {sum3} –≥—Ä–Ω')

portion_sweet_red_pepper = pw_input(f'–í–≤–µ–¥—ñ—Ç—å –±–∞–∂–∞–Ω—É –ø–æ—Ä—Ü—ñ—é –ø–µ—Ä—Ü—é üå∂, –≥—Ä–∞–º '
                               f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å {ENERGY_SWEET_RED_PEPPER} –∫–ö–∞–ª/100–≥—Ä', type=FLOAT)
sum4 = ONE_KILO_CALORIE * ENERGY_SWEET_RED_PEPPER + sum3
portion_sweet_red_pepper_energy = (float(portion_sweet_red_pepper) * ENERGY_SWEET_RED_PEPPER) / 100
total_energy = total_energy + portion_sweet_red_pepper_energy
put_success(f'–í–∏ –∑–∞–∫–∞–∑–∞–ª–∏ {portion_sweet_red_pepper} –≥—Ä–∞–º—ñ–≤ –ø–µ—Ä—Ü—é üå∂,\n'
            f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –ø–æ—Ä—Ü—ñ—ñ {portion_sweet_red_pepper}\n'
            f'–ó–∞–≥–∞–ª—å–Ω–∞ –Ω–∞–∫–æ–ø–∏—á–µ–Ω–∞ –∫–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {total_energy},\n'
            f'–í—Å—å–æ–≥–æ –¥–æ —Å–ø–ª–∞—Ç–∏ {sum4} –≥—Ä–Ω')

portion_parsley = pw_input(f'–í–≤–µ–¥—ñ—Ç—å –±–∞–∂–∞–Ω—É –ø–æ—Ä—Ü—ñ—é –ø—É—á–∫–æ–≤ –ø–µ—Ç—Ä—É—à–∫–∏ ü•¨, –≥—Ä–∞–º '
                               f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å {ENERGY_PARSLEY} –∫–ö–∞–ª/100–≥—Ä', type=FLOAT)
sum5 = ONE_KILO_CALORIE * ENERGY_PARSLEY + sum4

portion_parsley_energy = (float(portion_parsley) * ENERGY_PARSLEY) / 100
total_energy = total_energy + portion_parsley_energy
put_success(f'–í–∏ –∑–∞–∫–∞–∑–∞–ª–∏ {portion_parsley} –ø—É—á–∫–æ–≤ –ø–µ—Ç—Ä—É—à–∫–∏ ü•¨,\n'
            f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –ø–æ—Ä—Ü—ñ—ñ {portion_parsley}\n'
            f'–ó–∞–≥–∞–ª—å–Ω–∞ –Ω–∞–∫–æ–ø–∏—á–µ–Ω–∞ –∫–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {total_energy},\n'
            f'–í—Å—å–æ–≥–æ –¥–æ —Å–ø–ª–∞—Ç–∏ {sum5} –≥—Ä–Ω')

portion_banana = pw_input(f'–í–≤–µ–¥—ñ—Ç—å –±–∞–∂–∞–Ω—É –ø–æ—Ä—Ü—ñ—é –±–∞–Ω–∞–Ω—ñ–≤ üçå, –≥—Ä–∞–º '
                               f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å {ENERGY_BANANA} –∫–ö–∞–ª/100–≥—Ä', type=FLOAT)
sum6 = ONE_KILO_CALORIE * ENERGY_BANANA + sum5
portion_banana_energy = (float(portion_banana) * ENERGY_BANANA) / 100
total_energy = total_energy + portion_banana_energy
put_success(f'–í–∏ –∑–∞–∫–∞–∑–∞–ª–∏ {portion_banana} –≥—Ä–∞–º—ñ–≤ –±–∞–Ω–∞–Ω—ñ–≤ üçå,\n'
            f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –ø–æ—Ä—Ü—ñ—ñ {portion_banana}\n'
            f'–ó–∞–≥–∞–ª—å–Ω–∞ –Ω–∞–∫–æ–ø–∏—á–µ–Ω–∞ –∫–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {total_energy},\n'
            f'–í—Å—å–æ–≥–æ –¥–æ —Å–ø–ª–∞—Ç–∏ {sum6} –≥—Ä–Ω')

portion_waffles = pw_input(f'–í–≤–µ–¥—ñ—Ç—å –±–∞–∂–∞–Ω—É –ø–æ—Ä—Ü—ñ—é –≤–∞—Ñ–ª—ñ–≤ üßá, –≥—Ä–∞–º '
                               f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å {ENERGY_WAFFLES} –∫–ö–∞–ª/100–≥—Ä', type=FLOAT)
sum7 = ONE_KILO_CALORIE * ENERGY_WAFFLES + sum6
portion_waffles_energy = (float(portion_waffles) * ENERGY_WAFFLES) / 100
total_energy = total_energy + portion_waffles_energy
put_success(f'–í–∏ –∑–∞–∫–∞–∑–∞–ª–∏ {portion_waffles} –≥—Ä–∞–º—ñ–≤ –≤–∞—Ñ–ª—ñ–≤ üßá,\n'
            f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –ø–æ—Ä—Ü—ñ—ñ {portion_waffles}\n'
            f'–ó–∞–≥–∞–ª—å–Ω–∞ –Ω–∞–∫–æ–ø–∏—á–µ–Ω–∞ –∫–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {total_energy},\n'
            f'–í—Å—å–æ–≥–æ –¥–æ —Å–ø–ª–∞—Ç–∏ {sum7} –≥—Ä–Ω')

portion_bread = pw_input(f'–í–≤–µ–¥—ñ—Ç—å –±–∞–∂–∞–Ω—É –ø–æ—Ä—Ü—ñ—é –±—É—Ö–∞–Ω–æ–∫ —Ö–ª—ñ–±–∞ üçû, –≥—Ä–∞–º '
                               f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å {ENERGY_BREAD} –∫–ö–∞–ª/100–≥—Ä', type=FLOAT)
sum8 = ONE_KILO_CALORIE * ENERGY_BREAD + sum7
portion_bread_energy = (float(portion_bread) * ENERGY_BREAD) / 100
total_energy = total_energy + portion_bread_energy
put_success(f'–í–∏ –∑–∞–∫–∞–∑–∞–ª–∏ {portion_bread} –≥—Ä–∞–º—ñ–≤ –±—É—Ö–∞–Ω–æ–∫ —Ö–ª—ñ–±–∞ üçû,\n'
            f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –ø–æ—Ä—Ü—ñ—ñ {portion_bread}\n'
            f'–ó–∞–≥–∞–ª—å–Ω–∞ –Ω–∞–∫–æ–ø–∏—á–µ–Ω–∞ –∫–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {total_energy},\n'
            f'–î–æ —Å–ø–ª–∞—Ç–∏ {sum8} –≥—Ä–Ω')

portion_pistachios = slider(f'–í–≤–µ–¥—ñ—Ç—å –±–∞–∂–∞–Ω—É –ø–æ—Ä—Ü—ñ—é —Ñ—ñ—Å—Ç–∞—à–æ–∫ ü•ú, –≥—Ä–∞–º '
                               f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å {ENERGY_PISTACHIOS} –∫–ö–∞–ª/100–≥—Ä', min_value=0, max_value=2500)
sum9 = ONE_KILO_CALORIE * ENERGY_PISTACHIOS + sum8
portion_pistachios_energy = (float(portion_pistachios) * ENERGY_PISTACHIOS) / 100
total_energy = total_energy + portion_pistachios_energy
put_success(f'–í–∏ –∑–∞–∫–∞–∑–∞–ª–∏ {portion_pistachios} –≥—Ä–∞–º—ñ–≤  —Ñ—ñ—Å—Ç–∞—à–æ–∫ ü•ú,\n'
            f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –ø–æ—Ä—Ü—ñ—ñ {portion_pistachios}\n'
            f'–ó–∞–≥–∞–ª—å–Ω–∞ –Ω–∞–∫–æ–ø–∏—á–µ–Ω–∞ –∫–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {total_energy},\n'
            f'–í—Å—å–æ–≥–æ –¥–æ —Å–ø–ª–∞—Ç–∏ {sum9} –≥—Ä–Ω')

portion_kefir = slider(f'–í–≤–µ–¥—ñ—Ç—å –±–∞–∂–∞–Ω—É –ø–æ—Ä—Ü—ñ—é –∫–µ—Ñ—ñ—Ä—É üç∂, –º—ñ–ª—ñ–ª–∏—Ç—Ä—ñ–≤ '
                               f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å {ENERGY_KEFIR} –∫–ö–∞–ª/100–º–ª', min_value=0, max_value=2500)
sum10 = ONE_KILO_CALORIE * ENERGY_KEFIR + sum9
portion_kefir_energy = (float(portion_kefir) * ENERGY_KEFIR) / 100
total_energy = total_energy + portion_kefir_energy
put_success(f'–í–∏ –∑–∞–∫–∞–∑–∞–ª–∏ {portion_kefir} –º—ñ–ª—ñ–ª–∏—Ç—Ä—ñ–≤  –∫–µ—Ñ—ñ—Ä—É üç∂,\n'
            f'–ö–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –ø–æ—Ä—Ü—ñ—ñ {portion_kefir}\n'
            f'–ó–∞–≥–∞–ª—å–Ω–∞ –Ω–∞–∫–æ–ø–∏—á–µ–Ω–∞ –∫–∞–ª–æ—Ä—ñ–π–Ω—ñ—Å—Ç—å –∑–∞–º–æ–≤–ª–µ–Ω–Ω—è: {total_energy},\n'
            f'–í—Å—å–æ–≥–æ –¥–æ —Å–ø–ª–∞—Ç–∏ {sum10} –≥—Ä–Ω')

put_success(f'–í—Å—å–æ–≥–æ –¥–æ —Å–ø–ª–∞—Ç–∏ {sum10} –≥—Ä–Ω')
